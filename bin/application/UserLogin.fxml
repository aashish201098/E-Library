<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserLoginController">
   <children>
      <AnchorPane layoutY="9.0" prefHeight="448.0" prefWidth="472.0">
         <children>
            <Label layoutX="193.0" layoutY="32.0" text="User Login">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="usernametf" layoutX="233.0" layoutY="104.0" promptText="UserName" />
            <PasswordField fx:id="passwordtf" layoutX="233.0" layoutY="182.0" promptText="Password" />
            <Button fx:id="login" layoutX="221.0" layoutY="250.0" mnemonicParsing="false" onAction="#userLogin" text="Login" />
            <Hyperlink fx:id="newuserlbl" layoutX="205.0" layoutY="353.0" onMouseClicked="#newUser" text="NewUser?" />
            <Hyperlink fx:id="adminloginlbl" layoutX="198.0" layoutY="390.0" onMouseClicked="#adminLogin" text="Admin Login" />
            <Label fx:id="statuslbl" layoutX="162.0" layoutY="304.0" prefHeight="30.0" prefWidth="149.0" textFill="#db1825" />
            <Label layoutX="97.0" layoutY="106.0" text="User name :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="102.0" layoutY="184.0" text="Password :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
